{
  "name": "atomixjs",
  "version": "0.0.1",
  "module": "index.ts",
  "type": "module",
  "scripts": {
    "build": "bun build index.ts --outfile=d.js --target=browser --minify-whitespace --minify-identifiers --minify-syntax",
    "dev": "bun build index.ts --outfile=d.js --target=browser --sourcemap=inline --watch",
    "buildExample": "bun build ./examples/script.ts --outfile=examples/script.js --target=browser --minify-whitespace --minify-identifiers --minify-syntax"
  },
  "devDependencies": {
    "bun-types": "latest"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {}
}
